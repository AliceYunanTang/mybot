<div class="user-index-container">

<h2><span>Users</span> Index</h2>
<div class="table-container">
  <table class="table table-hover">

    <% @all_users.each do |u| %>
    <tr>
      <td>
        <%= link_to u.name, user_path(u.id) %>
      </td>
      <td>
        <%= u.email %>
      </td>
    </tr>
    <% end %>

  </table>
</div>


</div>
